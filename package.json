{
  "name": "vf-shelter-support-center",
  "version": "0.2.0",
  "scripts": {
    "node": "node",
    "ts": "npm run node -- -r ts-node/register",
    "build": "tsc",
    "cdk": "npm run build && cdk",
    "cdk:direct": "npm run cdk -- --app \"npx ts-node --prefer-ts-exts bin/direct.ts\"",
    "presynth": "rimraf cdk.out && mkdirp cdk.out",
    "synth": "npm run cdk -- synth > cdk.out/template.yml",
    "bootstrap": "npm run ts scripts/bootstrap.ts",
    "diff:direct": "npm run cdk:direct -- diff",
    "pub": "npm run ts scripts/publishToCodeArtifact",
    "synth:direct": "npm run cdk:direct -- synth --quiet",
    "deploy": "npm run cdk -- deploy --all --require-approval never`",
    "deploy:direct": "npm run cdk:direct -- deploy",
    "destroy": "npm run cdk -- destroy",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "migrate:extract-dev": "npx ts-node bin/migrate.ts extract --env dev --data-path ./resources/dev",
    "migrate:publish-test": "npx ts-node bin/migrate.ts publish --env test --data-path ./resources/dev",
    "migrate:extract-test": "npx ts-node bin/migrate.ts extract --env test --data-path ./resources/test"
  },
  "devDependencies": {
    "@aws-cdk/pipelines": "1.180.0",
    "@aws-sdk/client-connect": "^3.200.0",
    "@aws-sdk/client-dynamodb": "^3.200.0",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@voicefoundry-cloud/cdk-resources": "9.5.2",
    "@voicefoundry-cloud/vf-deploy": "^1.0.9",
    "@voicefoundry-cloud/vf-logger": "^0.1.8",
    "@voicefoundry-cloud/vf-omp": "6.2.0",
    "aws-cdk": "2.54.0",
    "aws-cdk-lib": "2.54.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1245.0",
    "constructs": "10.0.0",
    "esbuild": "^0.15.12",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.2",
    "mkdirp": "^1.0.4",
    "pino": "^8.7.0",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engineStrict": true,
  "engines": {
    "node": "^16.0.0",
    "npm": ">=8.0.0",
    "git": ">=2.2.3"
  }
}
